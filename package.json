{
  "name": "oam-api-next",
  "version": "0.0.1",
  "description": "API for next iteration of the OpenAerialMap browser",
  "main": "dist/server.js",
  "scripts": {
    "watch-server": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/server.ts",
    "build-ts": "tsc",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve": "node dist/server.js",
    "build": "npm run build-ts && npm run copy-static-assets",
    "start": "npm run serve",
    "test": "ts-node -P test/tsconfig.json test/**.ts"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "author": "Sean Harkins",
  "license": "MIT",
  "homepage": "https://github.com/sharkinsspatial/oam-api-next",
  "repository": "github:sharkinsspatial/oam-api-next",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/koa": "2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-router": "^7.0.28",
    "@types/koa__cors": "^2.2.2",
    "@types/node": "^10.7.0",
    "@types/shelljs": "^0.8.0",
    "nodemon": "^1.17.4",
    "shelljs": "^0.8.2",
    "sinon": "^7.0.0",
    "tape": "^4.9.1",
    "tape-await": "^0.1.1",
    "ts-mock-imports": "^1.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@types/polylabel": "^1.0.5",
    "class-validator": "^0.9.1",
    "dotenv": "^6.0.0",
    "geojson": "^0.5.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^3.3.2",
    "koa-router": "^7.4.0",
    "pg": "^7.4.3",
    "pg-connection-string": "^2.0.0",
    "polylabel": "^1.0.2",
    "reflect-metadata": "^0.1.12",
    "winston": "^3.0.0"
  }
}
